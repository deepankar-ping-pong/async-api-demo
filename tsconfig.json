{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "allowJs": true,
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "app"
  ]
}