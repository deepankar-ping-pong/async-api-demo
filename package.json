{
  "name": "async-api-demo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^9",
    "express": "^4",
    "solclientjs": "^10.18.2"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.11.0",
    "@cap-js/sqlite": "^2",
    "@types/node": "^22.0.0",
    "tsx": "^4",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "local": "tsc && cds watch"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "com.mycompany.User": {
        "kind": "odata",
        "model": "srv/external/User Creation-0.1.0-default"
      },
      "solace": {
        "kind": "solace",
        "impl": "srv/solace.js",
        "credentials": {
          "hostName": "wss://<solace host name>:443",
          "vpnName": "<solace vpn name>",
          "userName": "<solace client username>",
          "password": "<solace client password>"
        }
      }
    }
  }
}